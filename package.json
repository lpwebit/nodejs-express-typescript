    "mocha": "^7.1.0",
    "nodemon": "^1.19.4",
    "nyc": "^15.0.0",
    "prettier": "~1.19.1",
    "rimraf": "~3.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.6.2",
